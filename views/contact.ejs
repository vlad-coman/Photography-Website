<% include partials/header %>
<link rel="stylesheet" type="text/css" href="/stylesheets/contact.css">
<% include partials/body %>
<% include partials/navbar %>

<section id="contact"  class="content-section text-center">
   <div class="contact-section">
      <div class="container">
         <h2 class="color">Contact Me  <i class="fa fa-envelope" aria-hidden="true"></i></h2>
         <div class="row">
            <div class="col-md-8 col-md-offset-2">
               <form class="form-horizontal" action="/contact" method="POST" >
                  <div class="form-group color ">
                     <label for="exampleInputName2">Name*</label>
                     <input type="text" class="form-control" name="name" id="exampleInputName2" placeholder="Your name" required="required">
                  </div>
                  <div class="form-group color">
                     <label for="exampleInputEmail2">Email*</label>
                     <input type="email" class="form-control" name="email" id="exampleInputEmail2" placeholder="email@gmail.com" required="required">
                  </div>
                  <div class="form-group color">
                     <label for="exampleInputText">Your Message*</label>
                     <textarea  class="form-control" name="message" placeholder="Your message" required="required"></textarea> 
                  </div>
                  <button type="submit" class="btn btn-default">Send Message</button>
               </form>
               <hr>
               <ul class="list-inline banner-social-buttons">
                  <li><a href="#" class="btn btn-info btn-lg"><i class="fa fa-twitter"> <span class="network-name">Twitter</span></i></a></li>
                  <li><a href="#" class="btn btn-primary btn-lg "><i class="fa fa-facebook"> <span class="network-name">Facebook</span></i></a></li>
                  <li><a href="#" class="btn btn-danger btn-lg"><i class="fa fa-youtube-play"> <span class="network-name">Youtube</span></i></a></li>
               </ul>
            </div>
         </div>
      </div>
   </div>
</section>
<script
   src="https://code.jquery.com/jquery-3.2.1.js"
   integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE="
   crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

